//https://leetcode.com/problems/linked-list-cycle/
/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public boolean hasCycle(ListNode head) {
        Set<ListNode> seen =new HashSet<>();
        ListNode temp=head;
        seen.add(temp);
        while(temp!=null)
        {
            if(seen.contains(temp.next))
                return true;
            seen.add(temp.next);
            temp=temp.next;
            
            
        }
        return false;
    }
}
