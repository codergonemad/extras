//https://leetcode.com/problems/maximum-subarray/submissions/
//TIME LIMIT EXCEEDED
class Solution {
    public int maxSubArray(int[] nums) {
        int sum=Integer.MIN_VALUE;
        for(int i=0;i<nums.length;i++)
        {
            int curr=0;
            for(int j=i;j<nums.length;j++)
            {
                curr+=nums[j];
                if(curr>sum)
            {
                sum=curr;
            }
            }
            
        }
        return sum;
        
    }
}
